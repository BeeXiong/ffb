                        <thead>
                            <tr>
                                <td>Player Name</td>
                                <td>Position</td>
                                <td>Team</td>
                                <td>Drafted By</td>
                                <td>Pick Number</td>
                            </tr>
                        </thead>
                        <tbody>
                            @if (Model.lastPick == null)
                            {
                                <tr>
                                    <td>No Player Has Been Selected</td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                </tr>
                            }
                            else
                            {
                                <tr>
                                    <td>@Model.lastPick.player.firstName @Model.lastPick.player.lastName</td>
                                    <td>@Model.lastPick.player.playerPosition.position</td>
                                    <td>@Model.lastPick.player.team.location.teamLocation @Model.lastPick.player.team.homeTeamName</td>
                                    <td>@Model.lastPick.fantasy_League_Detail.user.UserName</td>
                                    <td>@Model.lastPick.draftPickNumber</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                    <div class="mx-2 my-4">
